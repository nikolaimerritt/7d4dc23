@{
    ViewData["Title"] = "Schedule";
}
<div class="container my-5">
    <h1 class="text-center">Event Schedule</h1>
    
    <div class="row">
        <div class="col-12">
            <table class="table table-bordered table-dark">
                <thead>
                    <tr>
                        <th>Time</th>
                        <th>Activity</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>09:30 – 10:00</td>
                        <td>Introduction: WithSecure to introduce objectives and rules</td>
                    </tr>
                    <tr>
                        <td>10:00 – 12:00</td>
                        <td>CTF starts: Participants, divided in teams, start working on the challenges</td>
                    </tr>
                    <tr>
                        <td>12:00 – 13:30</td>
                        <td>Lunch break: Participants take a break for lunch</td>
                    </tr>
                    <tr>
                        <td>13:30 – 16:00</td>
                        <td>CTF resumes: Participants resume working on the challenges</td>
                    </tr>
                    <tr>
                        <td>16:00 – 16:15</td>
                        <td>End of CTF: Wrap-up, winner announcement and Q&A</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>